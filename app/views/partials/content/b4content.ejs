<article>
        <% b4.forEach(function(item) { %>
            <div>
                <h3><%= item.name %></h3>
                <p>
                    Gender: <%= item.gender %><br>
                    Age: <%= item.age %><br>
                    Position: <%= item.position %>
                </p>
            </div>
        <% }); %>
      </article>
      